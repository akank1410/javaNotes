  TC : O(n)
   int s = 0, e=matrix.length-1, mid;
            while(s<=e)
            {
                mid = (s+e)/2;
                if(matrix[index][mid] == target)
                    return true;
                else if(matrix[index][mid] > target)
                e = mid-1; 
                else 
                s = mid+1;
            }
